{"html": "<html>\n                <head>\n                <style>\n                @page {size: A4; orientation: 1; margin: 0; padding: 0; }\n                                body {margin: 0; padding: 0; font-family: /* \"Open Sans\", */ Arial, Helvetica, sans-serif; line-height:1.3em; }\n                                .page { color:#000000; font-size:12px; position:relative; /* border:1px solid #f00; */ }\n                .present{\n                    height: 100%;\n                    overflow-y: auto;\n                    margin-top: 0;\n                    padding: 24px;\n                    box-sizing: border-box;\n                }\n\n                .table {\n                    display: flex;\n                    flex-flow: column nowrap;\n                    font-size: .8rem;\n                    margin: 0.5rem;\n                    line-height: 1.5;\n                    flex: 1 1 auto;\n                }\n\n                .table div{\n                    box-sizing: border-box;\n                }\n\n                .th {\n                    display: none;\n                    font-weight: 700;\n                }\n\n                .th:first-child {\n                    border-top: 1px solid #1c313a;\n                }\n\n\n                .th > .td {\n                    white-space: nowrap;\n                    text-overflow: ellipsis;\n                    justify-content: center;\n                }\n\n\n                .th .td:first-child, .tr.category .td:first-child{\n                    width: 24px;\n                    max-width: 24px;\n                    box-sizing: border-box;\n                    border-right: none;\n                }\n\n                .tr {\n                    position: relative;\n                    width: 100%;\n                    display: flex;\n                    flex-flow: row nowrap;\n                    z-index: 1;\n                    height: 24px;\n                    border-right : 1px solid black;\n                    border-bottom : 1px solid #d0d0d0;\n                }\n\n                .tr:not(.th):not(.category)::after {\n                    display: block;\n                    content: '';\n                    position: absolute;\n                    top: 0;\n                    left: 0;\n                    height: 100%;\n                    width: 100%;\n                    z-index: 0;\n                }\n\n                .tr:not(.td):nth-child(odd)::after {\n                    opacity: 0.6;\n                    transition: all .24s cubic-bezier(0.075, 0.82, 0.165, 1);\n                }\n\n                .tr:not(.td):nth-child(even)::after {\n                    opacity: 0.8;\n                    transition: all .24s cubic-bezier(0.075, 0.82, 0.165, 1);\n                }\n\n                .tr:not(.td):hover::after {\n                    opacity: 1;\n                }\n\n                .td {\n                    position: relative;\n                    display: flex;\n                    flex-flow: row nowrap;\n                    align-items: center;\n                    justify-content: center;\n                    flex-grow: 1;\n                    flex-basis: 0;\n                    padding: 0.5em;\n                    overflow: hidden;\n                    min-width: 0px;\n                    z-index: 2;\n                    word-break: break-word;\n                    white-space: nowrap;\n                    border-right: 1px #000000;\n                    font-size: 13px;\n                }\n\n                .td span{\n                    text-overflow: ellipsis;\n                    width: 100%;\n                    overflow: hidden;\n                    text-align: center;\n                }\n\n                .td span.left{\n                    text-align: left;\n                }\n\n                .td-input{\n                    position: relative;\n                    text-align: center;\n                    border-left : 1px dotted #d0d0d0;\n                    height: 100%;\n                }\n\n\n                @keyframes inputShadows{\n                    from{ box-shadow: inset 0 0 0 rgba(0, 0, 0, .2); }\n                    to{ box-shadow: inset 0 0 50px rgba(0, 0, 0, .2); }\n                }\n\n                .td:first-child {\n                    border-left: 1px solid #000000;\n                }\n\n                .td--comments, .td--medicine, .td--category {\n                    justify-content: flex-start;\n                }\n\n                .td--category iron-icon {\n                    height: 14px;\n                    width: 14px;\n                    margin-right: 4px;\n                }\n\n                .th .td {\n                    border-bottom: 1px solid #000000;\n                }\n\n                .span-3 {\n                    padding: 6px 19px; /* 19px because we had the padding of the missings .td padding + their borders */\n                }\n\n                .table .category {\n                    border-bottom: 1px solid #d0d0d0;\n                }\n\n                .reimbursed{\n                    width: 24px;\n                    max-width: 24px;\n                    box-sizing: border-box;\n                    padding: 0;\n                    border-right: none!important;\n                }\n\n                .reimbursed .reimbursed-status {\n                    height: 14px;\n                    width: 14px;\n                    padding: 2px;\n                    border-radius: 50%;\n                    box-shadow: 0 0 0 rgba(0, 0, 0, .2);\n                    transition: all .24s cubic-bezier(0.075, 0.82, 0.165, 1);\n                    z-index: 3;\n                }\n\n                .reimbursed .reimbursed-status:hover {\n                    height: 16px;\n                    width: 16px;\n                    box-shadow: 0 0 4px rgba(0, 0, 0, .2);\n                }\n\n                .reimbursed .reimbursed-status.hidden{\n                    display: none;\n                }\n\n                .reimbursed .reimbursed-status.ok {\n                   color: #07f87f;\n                }\n\n                .reimbursed .reimbursed-status.nok {\n                    color: #ff4d4d;\n                }\n\n                .reimbursed .reimbursed-status.pending {\n                    color: #fcdf35;\n                }\n\n                </style>\n                </head>\n                <body><div class=\"page\"><div class=\"present\">\n                    <div class=\"table\">\n                        <div class=\"tr th\">\n                            <div class=\"td reimbursed\"></div>\n                            <div class=\"td\"style=\"flex-grow: 5;\"></div>\n                            <div class=\"td\" style=\"flex-grow: 2;\"></div>\n                            <div class=\"td span-3\" style=\"flex-grow: 3;\">\n                                <span>{{localize.mom_break}}</span>\n                            </div>\n                            <div class=\"td span-3\" style=\"flex-grow: 3;\">\n                                <span>{{localize.mom_lunch}}</span>\n                            </div>\n                            <div class=\"td span-3\" style=\"flex-grow: 3;\">\n                                <span>{{localize.mom_dinner}}</span>\n                            </div>\n                            <div class=\"td\" style=\"flex-grow: 2;\"></div>\n                            <div class=\"td\" style=\"flex-grow: 4;\"></div>\n                        </div>\n                        <div class=\"tr th\">\n                            <div class=\"td\"></div>\n                            <div class=\"td td--medicine\" style=\"flex-grow: 5; justify-content: flex-start\">\n                                <span class=\"left\">{{localize.medic}}</span>\n                            </div>\n                            <div class=\"td\" style=\"flex-grow: 2;\">\n                                <span>{{localize.freq}}</span>\n                            </div>\n                            <div class=\"td bf-bef\">\n                                <span>{{localize.mom_before}}</span>\n                            </div>\n                            <div class=\"td bf-dur\">\n                                <span>{{localize.mom_during}}</span>\n                            </div>\n                            <div class=\"td bf-aft\">\n                                <span>{{localize.mom_after}}</span>\n                            </div>\n                            <div class=\"td lu-bef\">\n                                <span>{{localize.mom_before}}</span>\n                            </div>\n                            <div class=\"td lu-dur\">\n                                <span>{{localize.mom_during}}</span>\n                            </div>\n                            <div class=\"td lu-aft\">\n                                <span>{{localize.mom_after}}</span>\n                            </div>\n                            <div class=\"td di-bef\">\n                                <span>{{localize.mom_before}}</span>\n                            </div>\n                            <div class=\"td di-dur\">\n                                <span>{{localize.mom_during}}</span>\n                            </div>\n                            <div class=\"td di-aft\">\n                                <span>{{localize.mom_after}}</span>\n                            </div>\n                            <div class=\"td\" style=\"flex-grow: 2;\">\n                                <span>{{localize.endDate}}</span>\n                            </div>\n                            <div class=\"td td-comments\" style=\"flex-grow: 4; justify-content: space-between;\">\n                                <span class=\"left\">{{localize.com}}</span>\n                            </div>\n                        </div>\n                        {{#list}}\n                            <div class=\"tr category\">\n                                <div class=\"td\"></div>\n                                <div class=\"td td--category\" style=\"flex-grow: 5;\">\n                                    <iron-icon icon=\"history\"></iron-icon><span class=\"left\"><b>{{localizedTitle}}</b></span>\n                                </div>\n                                <div class=\"td\" style=\"flex-grow: 2;\"></div>\n                                <div class=\"td\"></div>\n                                <div class=\"td\"></div>\n                                <div class=\"td\"></div>\n                                <div class=\"td\"></div>\n                                <div class=\"td\"></div>\n                                <div class=\"td\"></div>\n                                <div class=\"td\"></div>\n                                <div class=\"td\"></div>\n                                <div class=\"td\"></div>\n                                <div class=\"td\" style=\"flex-grow: 2;\"></div>\n                                <div class=\"td\" style=\"flex-grow: 4;\"></div>\n                            </div>\n                            {{#meds}}\n                                <div class=\"tr\">\n                                    <div class=\"td reimbursed\">\n                                        <iron-icon id=\"reimbursedStatus\" icon=\"icons:euro-symbol\" class$=\"reimbursed-status {{hasValidChap4Class}}\"></iron-icon>\n                                        <paper-tooltip for=\"reimbursedStatus\">{{chap4Status}}</paper-tooltip>\n                                    </div>\n                                    <div class=\"td td--medicine\" style=\"flex-grow: 5;\">\n                                        <span class=\"left\">{{serviceDescription}}</span>\n                                    </div>\n                                    <div class=\"td\" style=\"flex-grow: 2;\"><span>{{servicePosology}}</span></div>\n                                    {{#combinedTableHeaders0}}\n                                        <div class=\"td td-input bf-bef\">\n                                            <span>{{administratedQuantity.quantity}}</span>\n                                        </div>\n                                    {{/combinedTableHeaders0}}\n                                    <div class=\"td td-input bf-bef\">{{regimenTable.1.0.administratedQuantity.quantity}}</div>\n                                    <div class=\"td td-input bf-dur\">{{regimenTable.2.0.administratedQuantity.quantity}}</div>\n                                    <div class=\"td td-input bf-aft\">{{regimenTable.3.0.administratedQuantity.quantity}}</div>\n                                    {{#combinedTableHeaders4}}\n                                        <div class=\"td td-input bf-bef\">\n                                            <span>{{administratedQuantity.quantity}}</span>\n                                        </div>\n                                    {{/combinedTableHeaders4}}\n                                    <div class=\"td td-input lu-bef\">{{regimenTable.5.0.administratedQuantity.quantity}}</div>\n                                    <div class=\"td td-input lu-dur\">{{regimenTable.6.0.administratedQuantity.quantity}}</div>\n                                    <div class=\"td td-input lu-aft\">{{regimenTable.7.0.administratedQuantity.quantity}}</div>\n                                    {{#combinedTableHeaders8}}\n                                        <div class=\"td td-input bf-bef\">\n                                            <span>{{administratedQuantity.quantity}}</span>\n                                        </div>\n                                    {{/combinedTableHeaders8}}\n                                    <div class=\"td td-input di-bef\">{{regimenTable.9.0.administratedQuantity.quantity}}</div>\n                                    <div class=\"td td-input di-dur\">{{regimenTable.10.0.administratedQuantity.quantity}}</div>\n                                    <div class=\"td td-input di-aft\">{{regimenTable.11.0.administratedQuantity.quantity}}</div>\n                                    {{#combinedTableHeaders12}}\n                                        <div class=\"td td-input bf-bef\">\n                                            <span>{{administratedQuantity.quantity}}</span>\n                                        </div>\n                                    {{/combinedTableHeaders12}}\n                                    <div class=\"td td-input\" style=\"flex-grow: 2;\"><span>{{getMedicationClosingDate}}</span></div>\n                                    <div class=\"td td-input\" style=\"flex-grow: 4;\"><span class=\"left\">{{medication.instructionForPatient}}</span></div>\n                                </div>\n                            {{/meds}}\n                        {{/list}}\n                    </div>\n                </div></div></body></html>"}
